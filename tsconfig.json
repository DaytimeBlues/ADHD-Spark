{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "module": "esnext",
    "skipLibCheck": true,
    "incremental": true,
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "exclude": ["e2e", "node_modules", "dist", "coverage"]
}
