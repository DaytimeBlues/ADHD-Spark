# AI API Keys - Copy to .env and fill in values
# NEVER commit the real .env file to git!

# Gemini API Key (Google AI)
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here

# Moonshot AI (Kimi 2.5) API Key
REACT_APP_MOONSHOT_API_KEY=your_moonshot_api_key_here

# AI Provider Configuration: 'vercel' | 'gemini-direct' | 'kimi-direct'
REACT_APP_AI_PROVIDER=vercel
