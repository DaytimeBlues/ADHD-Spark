# AI API Keys - Copy to .env and fill in values
# NEVER commit the real .env file to git!

# Gemini API Key (Google AI)
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# Moonshot AI (Kimi 2.5) API Key
EXPO_PUBLIC_MOONSHOT_API_KEY=your_moonshot_api_key_here

# AI Provider Configuration: 'vercel' | 'gemini-direct' | 'kimi-direct'
EXPO_PUBLIC_AI_PROVIDER=vercel

# AI Request timeout in milliseconds (default: 8000)
EXPO_PUBLIC_AI_TIMEOUT=8000

# Maximum AI request retry attempts (default: 3)
EXPO_PUBLIC_AI_MAX_RETRIES=3

# Google OAuth Client IDs (for Google Tasks/Calendar sync)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id_here
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id_here

# API Base URL (default: https://spark-adhd-api.vercel.app)
EXPO_PUBLIC_API_BASE_URL=https://spark-adhd-api.vercel.app
